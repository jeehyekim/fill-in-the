<div class="container-fluid">
  <div class="row text-center content-row">
  	<% @quiz.each do |quiz| %>
      <div class="col-md-3 col-sm-6 all-quizzes">
        <i class="fa fa-laptop fa-4x"></i>
        <h2><%= link_to quiz.title, quiz_path(quiz) %></h2>
        <p><%= quiz.content.first(80) %>...</p>
      </div>
    <% end %>
  </div>
</div>
    <%= will_paginate @quiz, class: "text-center col-xs-12 col-sm-6 col-sm-offset-3 quiz-nav" %>